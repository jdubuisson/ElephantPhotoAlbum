{% extends 'MopaBootstrapBundle::base.html.twig' %}

 {% block head_style %}
     <meta http-equiv="X-UA-Compatible" content="IE=edge" />
     {% stylesheets '@ElephantWebsiteBundle/Resources/public/css/*'%}
     <link rel="stylesheet" href="{{ asset_url }}"/>
     {% endstylesheets %}

     {% stylesheets
     output='fonts/glyphicons-halflings-regular.ttf'
     'bundles/elephantwebsite/fonts/glyphicons-halflings-regular.ttf' %}{% endstylesheets %}
     {% stylesheets
     output='fonts/glyphicons-halflings-regular.eot'
     'bundles/elephantwebsite/fonts/glyphicons-halflings-regular.eot' %}{% endstylesheets %}
     {% stylesheets
     output='fonts/glyphicons-halflings-regular.svg'
     'bundles/elephantwebsite/fonts/glyphicons-halflings-regular.svg' %}{% endstylesheets %}
     {% stylesheets
     output='fonts/glyphicons-halflings-regular.woff'
     'bundles/elephantwebsite/fonts/glyphicons-halflings-regular.woff' %}{% endstylesheets %}
     {% stylesheets
     output='fonts/glyphicons-halflings-regular.woff2'
     'bundles/elephantwebsite/fonts/glyphicons-halflings-regular.woff2' %}{% endstylesheets %}
 {% endblock head_style %}

{% block foot_script_assetic %}
    {% javascripts
    '@ElephantWebsiteBundle/Resources/public/js/jquery-1.11.2.min.js'
    '@ElephantWebsiteBundle/Resources/public/js/*' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock foot_script_assetic %}

{% block title %}Elephant Photo Album{% endblock title %}

{% block navbar %}
    {% include 'ElephantWebsiteBundle::navbar.html.twig' %}
{% endblock navbar %}

{% block body_end %}
{% endblock body_end %}


